def consonant_count(s):
    consonants = 'bcdfghjklmnpqrstvwxyzBCDFGHJKLMNPQRSTVWXYZ'
    count = 0
    
    for char in s:
        if char in consonants:
            count += 1
            
    return count


def consonant_count_comprehension(s):
    return sum(c in "bcdfghjklmnpqrstvwxyz" for c in s.lower())    
